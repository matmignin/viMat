        {"description": "double press fn-s",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "a",
                            "modifiers": [
                                "left_alt"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "s_skey pressed",
                            "value": 1
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "s_skey pressed",
                                "value": 1
                            }
                        },
                                                {
                            "key_code": "a",
                            "modifiers": [
                                "left_option",
                                "left_command"
                            ]
                        }
                    ],
                    "to_delayed_action": {
                        "to_if_invoked": [
                            {
                                "set_variable": {
                                    "name": "s_skey pressed",
                                    "value": 0
                                }
                            }
                        ],
                        "to_if_canceled": [
                            {
                                "set_variable": {
                                    "name": "s_skey pressed",
                                    "value": 0
                                }
                            }
                        ]
                    }
                }
            ]
        },
